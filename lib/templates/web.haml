%html
  %table(cellspacing='5' border = '1')
    %tbody
    - movies.each do |movie|
      %tr
        %td>= - "#{p movie.name} (#{movie.translate})"
        %td>= movie.producer
        %td>= movie.actors
        %td>= movie.rate
        %td>= movie.nokbudget
        %td
          %img{:src => "#{movie.picture}"}


